TMP="conf.tmp"

# check for compiler options
head -1 conf-cc | grep -e '-DTLS' conf-cc >/dev/null
if [ $? -eq 0 ] ; then TLS="Yes" ; else TLS="No" ; fi
head -1 conf-cc | grep -e '-DINET6' conf-cc >/dev/null
if [ $? -eq 0 ] ; then IP6="Yes" ; else IP6="No" ; fi

cat > $TMP << EOF

# !!! WARNING !!! WARNING !!! WARNING !!! WARNING !!!
# ******** Compiled-in values - DO NOT EDIT! ********
# These values were generated during the compilation,
# changing them here could lead to unforeseen errors!
HOMEDIR=`head -1 conf-qmail`
BREAK=`head -1 conf-break`
PATRN=`head -1 conf-patrn`
SPAWN="$fd_spn"
SPLIT=`head -1 conf-split`
UIDA="$UIDA"
UIDD="$UIDD"
UIDL="$UIDL"
UIDO="$UIDO"
UIDP="$UIDP"
UIDQ="$UIDQ"
UIDR="$UIDR"
UIDS="$UIDS"
GIDQ="$GIDQ"
GIDN="$GIDN"
TLS="$TLS"
IP6="$IP6"
# ************ end autogenerated values *************
EOF
